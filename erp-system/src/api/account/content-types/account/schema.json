{
  "kind": "collectionType",
  "collectionName": "accounts",
  "info": {
    "singularName": "account",
    "pluralName": "accounts",
    "displayName": "Account"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "asset",
        "liability",
        "revenue",
        "expense"
      ]
    },
    "transactions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction.transaction",
      "mappedBy": "account"
    }
  }
}
