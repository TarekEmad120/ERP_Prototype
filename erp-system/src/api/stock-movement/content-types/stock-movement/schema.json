{
  "kind": "collectionType",
  "collectionName": "stock_movements",
  "info": {
    "singularName": "stock-movement",
    "pluralName": "stock-movements",
    "displayName": "StockMovement"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "stock_movements"
    },
    "movement_type": {
      "type": "enumeration",
      "enum": [
        "inbound",
        "outbound",
        "adjustment"
      ]
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "movement_date": {
      "type": "datetime"
    },
    "warhouse": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::warhouse.warhouse",
      "inversedBy": "stock_movements"
    }
  }
}
