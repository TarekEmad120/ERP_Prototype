{
  "kind": "collectionType",
  "collectionName": "transactions",
  "info": {
    "singularName": "transaction",
    "pluralName": "transactions",
    "displayName": "Transaction"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "datetime"
    },
    "description": {
      "type": "text",
      "required": true,
      "unique": false
    },
    "amount": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "account": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::account.account",
      "inversedBy": "transactions"
    },
    "employee": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::employee.employee",
      "inversedBy": "transactions"
    },
    "invoice": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::invoice.invoice",
      "mappedBy": "transaction"
    }
  }
}
